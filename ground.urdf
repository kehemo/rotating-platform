<?xml version="1.0"?>
<robot name="ground">
  <link name="ground1">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -5"/>
      <geometry>
        <box size="100 100 10"/>
      </geometry>
      <material name="desert_sand">
        <color rgba="0.9297 0.7930 0.6758 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -5"/>
      <geometry>
        <box size="100 100 10"/>
      </geometry>
    </collision>
  </link>
  <link name="ground">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" ixy="0.5" ixz="0.5" iyy="1" iyz="0.5" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 1"/>
      <geometry>
        <box size="5 1 0.1"/>
      </geometry>
      <material name="greeny">
      	<color rgba="0.1953 0.6563 0.3203 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 1"/>
      <geometry>
        <box size="5 1 0.1"/>
      </geometry>
    </collision>
  </link>
  <link name="pole1">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="-2.5 0 0.5"/>
      <geometry>
        <cylinder radius="0.01" length="1"/>
      </geometry>
    </visual>
  </link>
  <link name="pole2">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="2.5 0 0.5"/>
      <geometry>
        <cylinder radius="0.01" length="1"/>
      </geometry>
    </visual>
  </link>
  <joint name="weld1" type="fixed">
    <origin xyz="0 0 0" />
    <parent link="world" />
    <child link="pole1" />
  </joint>
  <joint name="weld2" type="fixed">
    <origin xyz="0 0 0" />
    <parent link="world" />
    <child link="pole2" />
  </joint>
  <joint name="rot1" type="continuous">
    <origin xyz="0 0 0" />
    <parent link="pole1" />
    <child link="ground" />
  </joint>
  <joint name="weld" type="fixed">
    <parent link="world" />
    <child link="ground1" />
  </joint>
</robot>